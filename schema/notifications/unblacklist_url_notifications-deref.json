{
    "openDxlApi": "0.1",
    "info": {
        "title": "Unblacklist URL notifications",
        "version": "0.0.1",
        "description": "Unblacklist URL notifications"
    },
    "events": {
        "/notification/unblacklist/url": {
            "description": "Unblacklist URL notification",
            "payload": {
                "properties": {
                    "url": {
                        "type": "string",
                        "description": "Uniform Resource Identifier, [RFC3986]."
                    }
                },
                "required": [
                    "url"
                ],
                "example": {
                    "url": "http://example.com"
                }
            },
            "otherFields": {
                "service_id": {
                    "description": "The identifier of the DXL service that is the source of the notification",
                    "required": false,
                    "example": "{32cd9168-338f-11e4-0d01-005056946833}"
                },
                "product_name": {
                    "description": "The product that is the source of the message",
                    "required": false,
                    "example": "McAfee Threat Intelligence Exchange (TIE)"
                },
                "product_version": {
                    "description": "The version of the product that is the source of the message",
                    "required": false,
                    "example": "3.0.0"
                }
            }
        }
    }
}
